{
  "extends": ["tslint-config-alloy", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["**/node_modules/**"]
  },
  "rules": {
    "semicolon": true,
    "eofline": false,
    "indent": [true, "spaces", 2],
    "no-empty-interface": false,
		"ordered-imports": false,
    "jsdoc-format": false,
    "no-redundant-jsdoc": false,
    "unified-signatures": false
  }
}
